<odoo>
    <template id="report_my_custom_pdf">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2 t-field="doc.name"/>
                <p>Date: <span t-field="doc.date"/></p>
                <p>Description: <span t-field="doc.description"/></p>
            </t>
        </t>
    </template>
    </odoo>
